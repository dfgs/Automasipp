<UserControl x:Class="Automasipp.Desktop.Views.test"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Automasipp.Desktop.Views"
             mc:Ignorable="d" 
              d:DesignWidth="800">
	<StackPanel Orientation="Horizontal" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}">
		<Grid Margin="-5,0,0,0">
			<Path Data="M0,0 100,0 110,15 100,30 0,30 10,15 0,0" Stroke="#55000000" Fill="#55FFFFFF"/>
			<TextBlock Width="80" Text="scenario fhdsfhsdu" Margin="15,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" TextTrimming="CharacterEllipsis" Foreground="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}"/>
		</Grid>
		<Grid Margin="-5,0,0,0">
			<Path Data="M0,0 100,0 110,15 100,30 0,30 10,15 0,0" Stroke="#55000000" Fill="#55FFFFFF"/>
			<TextBlock Width="80" Text="scenario " Margin="15,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" TextTrimming="CharacterEllipsis" Foreground="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}" />
		</Grid>
	</StackPanel>
</UserControl>
